{
  "name": "@esal/admin-portal",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3004",
    "build": "tsc && vite build",
    "build:npm": "npm install && npm run build",
    "build:render": "cd ../.. && npm install --include=dev && cd packages/ui && npm install --include=dev && npm run build && cd ../../apps/admin-portal && npm install --include=dev && npm run build:vite-only",
    "build:vite-only": "vite build",
    "vercel-build": "cd ../.. && pnpm install && pnpm --filter @esal/ui run build && cd apps/admin-portal && pnpm run build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@esal/ui": "file:../../packages/ui",
    "lucide-react": "^0.513.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "vite": "^6.0.1"
  }
}
